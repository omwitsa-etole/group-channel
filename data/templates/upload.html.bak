{% extends "base2.html" %}
{% load rest_framework %}
{% block title %}
	<title>HOME|UPLOAD</title>
{% endblock %}
{% block content %}
<div class="alert_success">
{% if new_video %}
	<div class="alert_content">
	<p>Video was Successfully Uploaded</p>
	</div>
{% endif%}
</div>
<form name = "form" enctype = "multipart/form-data" action = "" method = "POST" >{% csrf_token %}
<div class="upload-video">
	{% csrf_token %}
		{% render_form serializer %}
	
	<br>
	<input type="submit" value="Upload" style="float:right;margin-right: 20%;padding: 9px;background: blue;border: none;">
</div>   
</form>
{%endblock%}